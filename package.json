{
  "name": "uncanny-mrincredible",
  "displayName": "Uncanny Mr. Incredible",
  "description": "In Your Face, shows you Incredible 'Uncanny Faces' that correlate to the number of errors in your code!",
  "version": "0.1.0",
  "publisher": "surajmandalcell",
  "homepage": "https://github.com/surajmandalcell",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/surajmandalcell/uncanny-mrincredible"
  },
  "engines": {
    "vscode": "^1.61.0"
  },
  "categories": [
    "Visualization"
  ],
  "icon": "assets/icon.png",
  "galleryBanner": {
    "color": "#191919",
    "theme": "dark"
  },
  "keywords": [
    "in your face",
    "visualization",
    "incredible",
    "ouch",
    "face",
    "errors",
    "error lens",
    "error count"
  ],
  "__metadata": {
    "publisherDisplayName": "surajmandalcell"
  },
  "activationEvents": [
    "onCommand:uncanny-mrincredible.openview.focus",
    "onView:uncanny-mrincredible.openview"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "custom-activitybar",
          "title": "Incredibly In Your Face",
          "icon": "assets/logo_bito.svg"
        }
      ]
    },
    "views": {
      "custom-activitybar": [
        {
          "type": "webview",
          "id": "uncanny-mrincredible.openview",
          "name": "In Your Face",
          "contextualTitle": "In Your Face"
        }
      ]
    },
    "commands": [
      {
        "command": "uncanny-mrincredible.openview.focus",
        "category": "In Your Face",
        "title": "Open Sidebar",
        "icon": "$(clear-all)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.18.21",
    "@types/vscode": "^1.61.0",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^7.32.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.1",
    "typescript": "^4.7.3"
  }
}
